#Sequence record: handles requests to the device.

record(seq, "$(P)Seq"){
   field (DESC, "Data Det 2")
   field (DOL1, "$(P)$(DET1):sString PP")
   field (DOL2, "$(P)$(DET2):sString PP")
   field (DOL3, "$(P)$(DET3):sString PP")
   field (DOL4, "$(P)$(DET4):sString PP")
   field (DOL5, "$(P)Fanout PP")
#   field (DOL5, "$(P)$(DET1):sBGM PP")
#   field (DOL6, "$(P)$(DET1):sThrM PP")
   field (DLY2, "0.25")
   field (DLY3, "0.25")
   field (DLY4, "0.25")
   field (DLY5, "0.25")
#   field (DLY6, "0.25")
   field (PINI, "YES")
   field (SCAN, "2 second")
}

record(fanout, "$(P)Fanout"){
   field (DESC, "Fanout for reading BG and Thr")
   field (LNK0, "$(P)$(DET1):sBGM PP")
   field (LNK1, "$(P)$(DET1):sThrM PP")
   field (LNK2, "$(P)$(DET2):sBGM PP")
   field (LNK3, "$(P)$(DET2):sThrM PP")
   field (LNK4, "$(P)$(DET3):sBGM PP")
   field (LNK5, "$(P)$(DET3):sThrM PP")
   field (LNK6, "$(P)$(DET4):sBGM PP")
   field (LNK7, "$(P)$(DET4):sThrM PP")
   field (SELM, "Specified")
   field (SELL, "$(P)Cycle PP")
}

record(calc, "$(P)Cycle"){
   field (DESC, "Cycles between 0 and 7")
   field (CALC, "(VAL=7)? 0 : (VAL+1)")
}

################# DET 1 ########################

# ACCUMULATED DOSE AND DOSE RATE

record(stringin, "$(P)$(DET1):sString") {
   field (DESC, "$(DET1)")
   field (DTYP, "stream")
   field (INP, "@dpu.proto get_data(1,0x41) $(PORT)")
}

record(ai, "$(P)$(DET1):sDoseRtM") {
   field (DESC, "Dose rate mantissa")
   field (DTYP, "stream")
   field (INP, "@dpu.proto get_dr(1,0x41,$(DET1)) $(PORT)")
   field (SCAN, "I/O Intr")
   field (FLNK, "$(P)$(DET1):sDoseRt")   
   field (SDIS, "$(P)$(DET1):sString.PACT NPP MS")
#   field (SDIS, "SDIS-Test CP MS")
   field (DISV, "-1") # Will never disable, but will propagate severity
}

